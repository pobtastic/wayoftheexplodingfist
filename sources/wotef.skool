; Creates the background image.
;
; Used by the routine at #R$5F22.
;
; .
;
; Each background uses 4 sets of data "pairs" and a single reference for attribute data (handled separately at
; #R$6010). For example, #R$602B(background 1) uses;
; #TABLE(default,centre,centre,centre)
; { =h Block | =h UDGs | =h Data }
; { 1 | #R$626A | #R$6102 }
; { 2 | #R$648A | #R$6157 }
; { 3 | #R$685A | #R$61D9 }
; { 4 | #R$6AE2 | #R$624D }
; { =h,c2 Attribute Data | #R$6061 }
; TABLE#
@label=CREATE_BACKGROUND
c$5F80 LD HL,$4080   ; {The first screen address is $4080 which allows space for the "header". This is the top of character block 4.
 $5F83 LD ($5F02),HL ; This is then stored in the buffer at #R$5F02.}
 $5F86 LD HL,$5F10   ; #REGhl=#R$5F10(Background address data buffer).
; Each background has 4 "sets" of data, so set a counter of 4.
 $5F89 LD B,$04      ; #REGb=Initialise block counter.
; The block processing loop.
@label=CREATE_BACKGROUND_LOOP
*$5F8B PUSH BC       ; Push the counter onto the stack.
; Each block contains two address references, one is for UDG data, and one is
; for positioning data.
 $5F8C LD E,(HL)     ; {Pick up the next background UDG data block and store it
 $5F8D INC HL        ; at #R$5F08.
 $5F8E LD D,(HL)     ;
 $5F8F INC HL        ;
 $5F90 LD ($5F08),DE ; }
 $5F94 LD E,(HL)     ; {Fetch the address of the next positioning data block
 $5F95 INC HL        ; and store it in #REGde.
 $5F96 LD D,(HL)     ;
 $5F97 INC HL        ; }
 $5F98 PUSH HL       ; Push the position of the address data buffer (pointing
                     ; to the start of the next block) onto the stack.
; Set up the current screen position, the positioning data and the UDG data
; block.
 $5F99 LD HL,($5F02) ; #REGhl=The current screen address from #R$5F02.
 $5F9C EX DE,HL      ; Switch #REGde and #REGhl so #REGde now holds the screen address and #REGhl holds the start of the
                     ;           positioning data block.
 $5F9D CALL $5FBE    ; Call #R$5FBE.
; fgg
 $5FA0 LD HL,($5F02) ; #REGhl=The current screen address from #R$5F02.
 $5FA3 LD BC,$0080   ; {Add $0080 to #REGhl.
 $5FA6 ADD HL,BC     ; }
 $5FA7 BIT 0,H       ; {Set the zero flag if we're at a screen section
 $5FA9 JR Z,$5FAF    ; boundary. Skip forward to #R$5FAF if not.}
 $5FAB LD A,$07      ; {Add $07 to the MSB of #REGhl to handle moving between
 $5FAD ADD A,H       ; screen sections.
 $5FAE LD H,A        ; }
@label=BACKGROUND_NO_BOUNDARY
*$5FAF LD ($5F02),HL ; Stash #REGhl at #R$5F02.
 $5FB2 POP HL        ; {Restores #REGhl and the counter from the stack.
 $5FB3 POP BC        ; }
 $5FB4 DJNZ $5F8B    ; Decrease counter by one and loop back to #R$5F8B until
                     ; counter is zero.
; Finally, now write the attributes and return.
 $5FB6 LD E,(HL)     ; {Fetch the last address from the #R$5F10(Background address data buffer) which is the attributes data and
 $5FB7 INC HL        ;           call #R$6010 to process and display in the attributes buffer.
 $5FB8 LD D,(HL)     ;
 $5FB9 EX DE,HL      ;
 $5FBA CALL $6010    ; }
 $5FBD RET           ; Return.
; Fetch the next positioning data and check for the terminator.
@label=BACKGROUND_FETCH_NEXT
*$5FBE LD A,(HL)     ; Fetch the next positioning data byte.
 $5FBF INC HL        ; Increase #REGhl by one to point to the next byte.
 $5FC0 CP (HL)       ; Check if this byte is the same as the previous
                     ; positioning data byte value.
 $5FC1 JR NZ,$5FC6   ; Jump to #R$5FC6 if the values are different.
 $5FC3 AND A         ; {Double zero is the terminator, so jump to #R$5FF3 to
 $5FC4 JR Z,$5FF3    ; return if this is detected.}
; Process the UDG data.
@label=BACKGROUND_PROCESS
*$5FC6 BIT 7,A       ; {Check if d7 is set, reset it regardless.
 $5FC8 RES 7,A       ; }
 $5FCA PUSH AF       ; Push the new positioning data value onto the stack.
 $5FCB EXX           ; Exchange the registers.
; Point #REGhl to the base address of the currently referenced UDG.
 $5FCC LD C,A        ; {Set the LSB of #REGbc to #REGa, and the MSB to $00.
 $5FCD LD B,$00      ; }
 $5FCF LD L,C        ; {Copy the same value into #REGhl.
 $5FD0 LD H,B        ; }
 $5FD1 ADD HL,BC     ; {* 8.
 $5FD2 ADD HL,HL     ;
 $5FD3 ADD HL,HL     ; }
 $5FD4 LD BC,($5F08) ; #REGbc=Fetch the address of the start of the UDG data
                     ; block from #R$5F08.
 $5FD8 ADD HL,BC     ; {Choose the referenced UDG and store this at #R$5F04.
 $5FD9 LD ($5F04),HL ; }
 $5FDC EXX           ; Restore the registers.
 $5FDD CALL $5FF4    ; Call #R$5FF4.
 $5FE0 CALL $6007    ; Call #R$6007.
; Check if the flag for repetition is set, if so - action it. If not, jump and
; work on the next position.
 $5FE3 POP AF        ; Fetch the position data off the stack, as this is #REGaf
                     ; this also restores the d7 check flag.
 $5FE4 JR Z,$5FBE    ; Jump back to #R$5FBE unless the bit was set.
; Using the following position data byte as a counter, copy the current byte
; this number of times.
 $5FE6 LD B,(HL)     ; Pick up the position data from #REGhl and store it in
                     ; #REGb to use as a counter.
 $5FE7 INC HL        ; Increment #REGhl to point to the next item of position
                     ; data.
 $5FE8 DEC B         ; Decrease the counter by one.
@label=BACKGROUND_REPEAT_COPY
*$5FE9 CALL $5FF4    ; Call #R$5FF4.
 $5FEC CALL $6007    ; Call #R$6007.
 $5FEF DJNZ $5FE9    ; Decrease counter by one and loop back to #R$5FE9 until
                     ; the counter is zero.
 $5FF1 JR $5FBE      ; Jump back round to #R$5FBE.
; Used to "just return" after flag checking operations.
@label=BACKGROUND_RETURN
*$5FF3 RET           ; Return.
; Copy a single UDG 8x8 block to the screen.
@label=COPY_UDG
*$5FF4 PUSH DE       ; {Push #REGde (current screen location) and #REGhl (next
 $5FF5 PUSH HL       ; position data location) onto the stack.}
 $5FF6 LD A,$08      ; Set a counter of $08 for the number of lines in a
                     ; character block.
 $5FF8 LD HL,($5F04) ; #REGhl=#R$5F04(the currently targeted UDG address).
@label=COPY_UDG_LOOP
*$5FFB EX AF,AF'     ; Stash the counter for later.
 $5FFC LD A,(HL)     ; {Copy the UDG data held by #REGhl into the screen buffer
 $5FFD LD (DE),A     ; currently pointed to by #REGde.}
 $5FFE INC HL        ; Increment #REGhl by one to point to the next line of the
                     ; UDG data.
 $5FFF INC D         ; Increment the MSB of #REGde by one to point to the line
                     ; directly below in the screen buffer.
 $6000 EX AF,AF'     ; Restore the counter.
 $6001 DEC A         ; Decrease the counter by one.
 $6002 JR NZ,$5FFB   ; Loop back to #R$5FFB until the counter is zero.
 $6004 POP HL        ; {Restore the original #REGhl and #REGde values for
 $6005 POP DE        ; screen location and position data from the stack.}
 $6006 RET           ; Return.
; Handles moving from one screen location block to the next.
@label=BKG_NEXT_SCREEN_BLOCK
*$6007 INC DE        ; Increase #REGde by 1.
 $6008 BIT 0,D       ; {Return if d0 is not set on the MSB of #REGde. This
 $600A RET Z         ; checks to see if we're at a screen boundary.}
 $600B LD A,$07      ; {Add $07 to #REGd, so that #REGde points to the next
 $600D ADD A,D       ; block down.
 $600E LD D,A        ; }
 $600F RET           ; Return.

; Unpack background attribute data.
;
; Used by the routine at #R$5F80.
;
; .
;
; Unpacks the attribute data into the buffer. On entry #REGhl will contain one of the following background
; reference memory locations;
; #TABLE(default,centre,centre)
; { =h Background reference | =h Background data }
; { $01 | #R$6061 }
; { $02 | #R$6B83 }
; { $03 | #R$7766 }
; TABLE#
; The idea here is to use bit 7 (flash) as a flag to signify to copy the attrbute byte a specified number of times. For
; example;
; #TABLE(default,centre,centre,centre,centre)
; { =h Address | =h Value | =h Binary | =h Value After Reset }
; { $6061 | $#N(#PEEK($6061)) | #EVAL(#PEEK($6061),2,8) | $#N(#EVAL(#PEEK$6061-128)) }
; { $6062 | $#N(#PEEK($6062)) |  |  }
; TABLE#
; So, in $6061 bit 7 is set - after a reset we take $6062 as a counter and hence copy $#N(#EVAL(#PEEK$6061-128)) into
; the following #PEEK($6062) memory locations.
@label=BACKGROUND_ATTRIBUTES
c$6010 LD DE,$5800   ; Begin at the start of the #R$5800.
@label=BACKGROUND_ATTR_LOOP
*$6013 LD A,(HL)     ; Pick up the attribute data from #REGhl.
 $6014 INC HL        ; Increase #REGhl by one.
 $6015 CP (HL)       ; {If the following byte is different to the current
 $6016 JR NZ,$601A   ; attribute byte jump to #R$601A.}
 $6018 AND A         ; {The data terminates with double zeroes, so if this
 $6019 RET Z         ; occurs then return.}
; Copy and loop back round to #R$6013 until bit 7 in the current attribute data
; is set.
@label=BKG_ATTR_REPEAT_COPY
*$601A BIT 7,A       ; {Test if d7 is set in current attribute data. Reset bit
 $601C RES 7,A       ; to zero.}
 $601E LD (DE),A     ; Write the attribute data to #REGde.
 $601F INC DE        ; Increment #REGde by one.
 $6020 JR Z,$6013    ; Jump back to #R$6013 unless the bit was set.
; Using the following attribute data byte as a counter, copy the current byte
; this number of times.
 $6022 LD B,(HL)     ; Pick up the attribute data from HL into #REGb to use as
                     ; a counter.
 $6023 INC HL        ; Increase #REGhl by one.
 $6024 DEC B         ; Decrease the counter by one.
@label=BACKGROUND_COPY_ATTR_REPEAT
*$6025 LD (DE),A     ; Write the same attribute data to #REGde.
 $6026 INC DE        ; Increment #REGde by one.
 $6027 DJNZ $6025    ; Decrease counter by one and loop back to #R$6025 until
                     ; counter is zero.
 $6029 JR $6013      ; Jump back to #R$6013.

