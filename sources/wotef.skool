; Screen Calculation.
;
; Used by the routines at #R$9200, #R$9255, #R$92E4, #R$BF13 and #R$C101.
;
; .
;
; This routine works out which area of the screen we are in.
;       Here are a few examples of usage;
;       #REGa=$22
;       #REGhl=$4000
;       #REGa=$22 (restored from #REGe)
;       AND $07 == #REGa=$02
;       #REGbc=$0200
;       #REGhl=$4200
;       #REGa=$22 (restored from #REGe)
;       AND $38 == #REGa=$20
;       #REGbc=$0020
;       #REGbc=$0080
;       #REGhl=$4280
@label=SCREEN_CALC
c$C203 LD E,A        ; Stash the accumulator in #REGe for later.
 $C204 AND $C0       ; Mask off low bits d6-d7.
 $C206 JP Z,$C21A    ; If the result is now zero, jump to #R$C21A.
 $C209 CP $40        ; {If #REGa is $40 then jump to #R$C214.
 $C20B JP Z,$C214    ; }
@label=SCREEN_BOTTOM
 $C20E LD HL,$5000   ; {Sets #REGhl to $5000 (the bottom of the screen buffer)
 $C211 JP $C21D      ; and jump to #R$C21D.}
@label=SCREEN_MIDDLE
*$C214 LD HL,$4800   ; Sets #REGhl to $4800 (the middle of the screen buffer)
                     ; and jump to #R$C21D.
 $C217 JP $C21D      ; Jump to #R$C21D.
@label=SCREEN_TOP
*$C21A LD HL,$4000   ; Sets #REGhl to $4000 (the top of the screen buffer) and
                     ; continue on...
; Now we have the base screen address; work out the exact address to return.
@label=SCREEN_CALC_MINOR
*$C21D LD A,E        ; Restore the accumulator from #REGe.
 $C21E AND $07       ; Mask bits d0-d2.
 $C220 LD B,A        ; {Push the accumulator into #REGbc as the high-order byte
 $C221 LD C,$00      ; (low is $00).}
 $C223 ADD HL,BC     ; Adds #REGbc to #REGhl.
 $C224 LD A,E        ; Again, restore the accumulator from #REGe.
 $C225 AND $38       ; Mask bits d3-d5.
 $C227 LD C,A        ; {Push the accumulator into #REGbc as the low-order byte
 $C228 LD B,$00      ; (high is $00).}
 $C22A SLA C         ; {* 2 * 2.
 $C22C RL B          ;
 $C22E SLA C         ;
 $C230 RL B          ; }
 $C232 ADD HL,BC     ; Adds #REGbc to #REGhl.
 $C233 RET           ; Return.

