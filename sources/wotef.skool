; Change Background.
;
; #HTML(Takes the background reference from #R$5F00 and executes the appropriate routine to copy the background data
;       addresses to the buffer at #R$5F10.
;       <div>Used by the routine at #R$9200.</div>)
;
; .
;
; Blanks the screen.
c$5F22 LD HL,$5800   ; {Writes $3F to all $300 locations of the attributes buffer.
 $5F25 LD E,L        ; #TABLE(default,centre,centre,centre,centre)
 $5F26 LD D,H        ; { =h Value | =h Ink | =h Paper | =h Bright }
 $5F27 INC DE        ; { $3F | $07 | $07 | $00 }
 $5F28 LD (HL),$3F   ; TABLE#
 $5F2A LD BC,$02FF   ; This is white ink on white paper.
 $5F2D LDIR          ; }
 $5F2F LD HL,$4000   ; {Writes $00 to all $1800 locations of the screen buffer.
 $5F32 LD E,L        ;
 $5F33 LD D,H        ;
 $5F34 INC DE        ;
 $5F35 LD BC,$17FF   ;
 $5F38 LD (HL),$00   ;
 $5F3A LDIR          ; }
; Fetch the background reference from #R$5F00 and use it to calculate the
; address for the copy-to-buffer routine.
 $5F3C LD HL,$5F00   ; {Loads #REGa with the contents of #R$5F00.
 $5F3F LD A,(HL)     ; }
 $5F40 DEC A         ; {Decrease it by one, then double it.
 $5F41 ADD A,A       ; }
 $5F42 LD C,A        ; {Store it as the LSB of #REGbc (MSB is $00).
 $5F43 LD B,$00      ; }
 $5F45 LD (HL),B     ; Write $00 to #R$5F00.
 $5F46 LD HL,$5F0A   ; #REGhl=The beginning of three address references for the copy routines which populate #R$5F10 at
                     ;           #R$5F0A($5F0A).
 $5F49 ADD HL,BC     ; Add #REGbc to #REGhl.
 $5F4A LD E,(HL)     ; {#REGde=One of the three background address references;
 $5F4B INC HL        ; #TABLE(default,centre,centre)
 $5F4C LD D,(HL)     ; { =h #R$5F00 | =h Address Reference }
                     ; { $01 | #R$5F0A($5F0A) }
                     ; { $02 | #R$5F0C($5F0C) }
                     ; { $03 | #R$5F0E($5F0E) }
                     ; TABLE#}
 $5F4D EX DE,HL      ; Swap #REGde and #REGhl.
 $5F4E CALL $5F52    ; Call #R$5F52.
 $5F51 RET           ; Return.
; Indirect jump to copy-to-buffer routine.
*$5F52 JP (HL)       ; Indirect jump to address held by #REGhl.
                     ; #TABLE(default,centre,centre)
                     ; { =h #R$5F00 | =h Jumps To... }
                     ; { $01 | #R$5F53 }
                     ; { $02 | #R$5F62 }
                     ; { $03 | #R$5F71 }
                     ; TABLE#
; Set up background 1.
 $5F53 LD HL,$602B   ; {Copies $12 bytes of data from #R$602B to #R$5F10.
 $5F56 LD DE,$5F10   ;
 $5F59 LD BC,$0012   ;
 $5F5C LDIR          ; }
 $5F5E CALL $5F80    ; Call #R$5F80.
 $5F61 RET           ; Return.
; Set up background 2.
 $5F62 LD HL,$603D   ; {Copies $12 bytes of data from #R$603D to #R$5F10.
 $5F65 LD DE,$5F10   ;
 $5F68 LD BC,$0012   ;
 $5F6B LDIR          ; }
 $5F6D CALL $5F80    ; Call #R$5F80.
 $5F70 RET           ; Return.
; Set up background 3.
